;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Rover Control App"
run/main_scene="res://MVVM/View/Startup.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
run/max_fps=60
run/low_processor_mode=true
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Resources/raptors_logoHorizontal_color_nobg.png"
boot_splash/fullsize=false
config/icon="res://Resources/icon.svg"

[autoload]

LocalSettings="res://Core/LocalSettings.cs"
MqttNode="res://MVVM/Model/MqttNode.cs"
PressedKeys="res://MVVM/Model/PressedKeys.cs"
MissionStatus="res://MVVM/Model/MissionStatus.cs"
RoverCommunication="res://MVVM/Model/RoverCommunication.cs"
MissionSetPoint="res://MVVM/Model/MissionSetPoint.cs"

[debug]

file_logging/enable_file_logging=true
file_logging/max_log_files=50

[display]

window/size/viewport_width=1154

[dotnet]

project/assembly_name="Rover Control App"

